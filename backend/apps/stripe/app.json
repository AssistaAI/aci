{
  "name": "STRIPE",
  "display_name": "Stripe",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/stripe.svg",
  "provider": "Stripe, Inc.",
  "version": "1.0.0",
  "description": "\"Process payments, manage subscriptions, handle refunds, and track finances effortlessly through chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "type": "oauth2",
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ STRIPE_CLIENT_ID }}",
      "client_secret": "{{ STRIPE_CLIENT_SECRET }}",
      "authorization_url": "https://connect.stripe.com/oauth/authorize",
      "token_url": "https://connect.stripe.com/oauth/token",
      "refresh_token_url": "https://connect.stripe.com/oauth/token",
      "scopes": {
        "read_write": "Full read and write access to account data",
        "read_only": "Read-only access to account data"
      }
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Payment Processing",
    "E-commerce",
    "Financial Services"
  ],
  "visibility": "public",
  "active": true
}