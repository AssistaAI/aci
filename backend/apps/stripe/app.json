{
  "name": "STRIPE",
  "display_name": "Stripe",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/stripe.svg",
  "provider": "Stripe, Inc.",
  "version": "1.0.0",
  "description": "\"Process payments, manage subscriptions, handle refunds, and track finances effortlessly through chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ STRIPE_CLIENT_ID }}",
      "client_secret": "{{ STRIPE_CLIENT_SECRET }}",
      "scope": "read_write",
      "authorize_url": "https://connect.stripe.com/oauth/authorize",
      "access_token_url": "https://connect.stripe.com/oauth/token",
      "refresh_token_url": "https://connect.stripe.com/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Payment Processing",
    "E-commerce",
    "Financial Services"
  ],
  "visibility": "public",
  "active": true
}