{
  "name": "YOUTUBE",
  "display_name": "YouTube",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/youtube.svg",
  "provider": "Google LLC",
  "version": "1.0.0",
  "description": "\"Search for videos, create playlists, and organize your favorite content effortlessly through chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_YOUTUBE_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_YOUTUBE_CLIENT_SECRET }}",
      "scope": "https://www.googleapis.com/auth/youtube",
      "authorize_url": "https://accounts.google.com/o/oauth2/v2/auth",
      "access_token_url": "https://oauth2.googleapis.com/token",
      "refresh_token_url": "https://oauth2.googleapis.com/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Media"
  ],
  "visibility": "public",
  "active": true
}