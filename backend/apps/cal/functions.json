  {
    "name": "CAL__BOOKINGS",
    "description": "Retrieve bookings from Cal.com calendar",
    "tags": [
      "booking",
      "appointments"
    ],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "GET",
      "path": "/v2/bookings",
      "server_url": "https://api.cal.com",
      "headers": {
        "cal-api-version": "2024-06-11"
      }
    },
    "parameters": {
      "type": "object",
      "properties": {
        "query": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string",
              "description": "Filter bookings by status",
              "enum": [
                "upcoming",
                "recurring",
                "past",
                "cancelled",
                "unconfirmed"
              ],
              "example": "upcoming"
            },
            "afterStart": {
              "type": "string",
              "description": "Filter bookings starting after this date (ISO 8601)",
              "format": "date-time",
              "example": "2024-01-01T00:00:00Z"
            },
            "beforeEnd": {
              "type": "string",
              "description": "Filter bookings ending before this date (ISO 8601)",
              "format": "date-time",
              "example": "2024-12-31T23:59:59Z"
            },
            "take": {
              "type": "integer",
              "description": "Number of bookings to return",
              "minimum": 1,
              "maximum": 100,
              "default": 10
            },
            "skip": {
              "type": "integer",
              "description": "Number of bookings to skip for pagination",
              "minimum": 0,
              "default": 0
            }
          },
          "required": [],
          "visible": [
            "status",
            "afterStart",
            "beforeEnd",
            "take",
            "skip"
          ],
          "additionalProperties": false
        }
      },
      "required": [],
      "visible": [
        "query"
      ],
      "additionalProperties": false
    }
  },
