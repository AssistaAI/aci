  {
    "name": "CAL__CREATE_EVENT_TYPE",
    "description": "Create a new event type in Cal.com",
    "tags": [
      "event-types",
      "calendar"
    ],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "POST",
      "path": "/v2/event-types",
      "server_url": "https://api.cal.com",
      "content_type": "application/json",
      "headers": {
        "cal-api-version": "2024-06-11"
      }
    },
    "parameters": {
      "type": "object",
      "properties": {
        "body": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "description": "Title of the event type",
              "example": "30-minute Meeting"
            },
            "slug": {
              "type": "string",
              "description": "URL slug for the event type",
              "example": "30min"
            },
            "description": {
              "type": "string",
              "description": "Description of the event type",
              "example": "A quick 30-minute meeting"
            },
            "length": {
              "type": "integer",
              "description": "Duration in minutes",
              "minimum": 1,
              "example": 30
            },
            "locations": {
              "type": "array",
              "description": "Meeting locations",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "description": "Location type",
                    "enum": [
                      "inPerson",
                      "link",
                      "integrations:zoom",
                      "integrations:google:meet"
                    ]
                  },
                  "address": {
                    "type": "string",
                    "description": "Physical address or meeting link"
                  }
                },
                "required": [
                  "type"
                ],
                "visible": [
                  "type",
                  "address"
                ],
                "additionalProperties": false
              }
            },
            "bookingFields": {
              "type": "array",
              "description": "Custom booking form fields",
              "items": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Field name"
                  },
                  "type": {
                    "type": "string",
                    "description": "Field type",
                    "enum": [
                      "name",
                      "email",
                      "phone",
                      "address",
                      "text",
                      "textarea",
                      "select",
                      "multiselect",
                      "number"
                    ]
                  },
                  "required": {
                    "type": "boolean",
                    "description": "Whether field is required"
                  }
                },
                "required": [
                  "name",
                  "type"
                ],
                "visible": [
                  "name",
                  "type",
                  "required"
                ],
                "additionalProperties": false
              }
            }
