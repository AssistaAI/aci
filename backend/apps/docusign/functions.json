  {
    "name": "DOCUSIGN__CREATE_ENVELOPE",
    "description": "Create a new envelope with documents and recipients for electronic signature collection.",
    "tags": ["envelope", "create", "signature", "documents"],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "POST",
      "path": "/restapi/v2.1/accounts/{account_id}/envelopes",
      "server_url": "https://demo.docusign.net"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "path": {
          "type": "object",
          "properties": {
            "account_id": {
              "type": "string",
              "description": "DocuSign account ID"
            }
          },
          "required": ["account_id"],
          "visible": ["account_id"],
          "additionalProperties": false
        },
        "header": {
          "type": "object",
          "properties": {
            "Content-Type": {
              "type": "string",
              "default": "application/json"
            }
          },
          "required": ["Content-Type"],
          "visible": [],
          "additionalProperties": false
        },
        "body": {
          "type": "object",
          "properties": {
            "emailSubject": {
              "type": "string",
              "description": "Subject line for the signature request email"
            },
            "emailMessage": {
              "type": "string",
              "description": "Message body for the signature request email"
            },
            "status": {
              "type": "string",
              "enum": ["draft", "sent"],
              "default": "sent",
              "description": "Initial status of the envelope"
            },
            "documents": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "documentBase64": {
                    "type": "string",
                    "description": "Base64 encoded document content"
                  },
                  "name": {
                    "type": "string",
                    "description": "Document name"
                  },
                  "fileExtension": {
                    "type": "string",
                    "description": "File extension (pdf, docx, etc.)"
                  },
                  "documentId": {
                    "type": "string",
                    "description": "Unique identifier for the document"
                  }
                },
                "required": ["documentBase64", "name", "documentId"],
                "visible": ["documentBase64", "name", "fileExtension", "documentId"]
              },
              "description": "Array of documents to include in the envelope"
            },
            "recipients": {
              "type": "object",
              "properties": {
                "signers": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string",
                        "format": "email",
                        "description": "Signer's email address"
                      },
                      "name": {
                        "type": "string",
                        "description": "Signer's full name"
                      },
                      "recipientId": {
                        "type": "string",
                        "description": "Unique identifier for the recipient"
                      },
                      "routingOrder": {
                        "type": "integer",
                        "default": 1,
                        "description": "Order in which recipient should sign"
                      }
                    },
                    "required": ["email", "name", "recipientId"],
                    "visible": ["email", "name", "recipientId", "routingOrder"]
                  },
                  "description": "Array of signers for the envelope"
                }
              },
              "required": ["signers"],
              "visible": ["signers"],
              "additionalProperties": false
            }
          },
          "required": ["emailSubject", "documents", "recipients"],
          "visible": ["emailSubject", "emailMessage", "status", "documents", "recipients"],
          "additionalProperties": false
        }
      },
      "required": ["path", "header", "body"],
      "visible": ["path", "body"],
      "additionalProperties": false
    }
  },
