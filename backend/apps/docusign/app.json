{
  "name": "DOCUSIGN",
  "display_name": "DocuSign",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/docusign.svg",
  "provider": "DocuSign, Inc.",
  "version": "2.1.0",
  "description": "\"Send documents for signatures, track approvals, and streamline signing processes effortlessly through chat.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ DOCUSIGN_CLIENT_ID }}",
      "client_secret": "{{ DOCUSIGN_CLIENT_SECRET }}",
      "scope": "signature impersonation",
      "authorize_url": "https://account.docusign.com/oauth/auth",
      "access_token_url": "https://account.docusign.com/oauth/token",
      "refresh_token_url": "https://account.docusign.com/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Digital Signatures",
    "Document Management",
    "Workflow",
    "Legal"
  ],
  "visibility": "public",
  "active": true
}