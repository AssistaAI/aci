  {
    "name": "HUBSPOT__CONTACT_PROPERTY_CHANGED",
    "description": "Triggered when a specific contact property is changed in HubSpot",
    "trigger_type": "contact.propertyChange",
    "protocol": "webhook",
    "protocol_data": {
      "subscription_api": "https://api.hubapi.com/webhooks/v3/{appId}/subscriptions",
      "event_type": "contact.propertyChange",
      "verification_method": "hmac_sha256",
      "signature_header": "X-HubSpot-Signature",
      "requires_property_name": true
    },
    "event_schema": {
      "type": "object",
      "properties": {
        "objectId": {
          "type": "integer",
          "description": "The ID of the contact"
        },
        "propertyName": {
          "type": "string",
          "description": "Name of the property that changed (e.g., 'email', 'firstname')"
        },
        "propertyValue": {
          "type": "string",
          "description": "New value of the property"
        },
        "eventId": {
          "type": "integer"
        },
        "subscriptionId": {
          "type": "integer"
        },
        "portalId": {
          "type": "integer"
        },
        "occurredAt": {
          "type": "integer"
        },
        "eventType": {
          "type": "string",
          "enum": ["contact.propertyChange"]
        }
      }
    },
    "filters": {
      "type": "object",
      "properties": {
        "property_name": {
          "type": "string",
          "description": "Specific property to watch (e.g., 'email', 'firstname', 'lastname')",
          "examples": ["email", "firstname", "lastname", "phone", "company"]
        }
      },
      "required": ["property_name"],
      "description": "REQUIRED: Must specify which property to watch for changes"
    },
    "visibility": "public",
    "active": true
  },
