[
  {
    "name": "HUBSPOT__CONTACT_CREATED",
    "description": "Triggered when a new contact is created in HubSpot",
    "trigger_type": "contact.creation",
    "protocol": "webhook",
    "protocol_data": {
      "subscription_api": "https://api.hubapi.com/webhooks/v3/{appId}/subscriptions",
      "event_type": "contact.creation",
      "verification_method": "hmac_sha256",
      "signature_header": "X-HubSpot-Signature"
    },
    "event_schema": {
      "type": "object",
      "properties": {
        "objectId": {
          "type": "integer",
          "description": "The ID of the contact that was created"
        },
        "eventId": {
          "type": "integer",
          "description": "Unique ID for this event"
        },
        "subscriptionId": {
          "type": "integer",
          "description": "ID of the subscription that triggered this webhook"
        },
        "portalId": {
          "type": "integer",
          "description": "HubSpot portal (account) ID"
        },
        "occurredAt": {
          "type": "integer",
          "description": "Timestamp (milliseconds) when the event occurred"
        },
        "eventType": {
          "type": "string",
          "enum": ["contact.creation"]
        }
      }
    },
    "filters": {
      "type": "object",
      "properties": {},
      "description": "No additional filters available for contact creation events"
    },
    "visibility": "public",
    "active": true
  },
