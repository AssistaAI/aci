[
  {
    "name": "MONDAY__GET_BOARDS",
    "description": "Retrieve boards from Monday.com workspace with their structure, items, and metadata.",
    "tags": ["boards", "workspace", "projects"],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "POST",
      "path": "/v2",
      "server_url": "https://api.monday.com"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "header": {
          "type": "object",
          "properties": {
            "Content-Type": {
              "type": "string",
              "default": "application/json"
            }
          },
          "required": ["Content-Type"],
          "visible": [],
          "additionalProperties": false
        },
        "body": {
          "type": "object",
          "properties": {
            "query": {
              "type": "string",
              "default": "query { boards(limit: $limit) { id name description state board_kind items { id name state } columns { id title type } } }",
              "description": "GraphQL query for retrieving boards"
            },
            "variables": {
              "type": "object",
              "properties": {
                "limit": {
                  "type": "integer",
                  "default": 25,
                  "maximum": 100,
                  "description": "Number of boards to return"
                }
              },
              "required": [],
              "visible": ["limit"],
              "additionalProperties": false
            }
          },
          "required": ["query"],
          "visible": ["variables"],
          "additionalProperties": false
        }
      },
      "required": ["header", "body"],
      "visible": ["body"],
      "additionalProperties": false
    }
  },
  {
    "name": "MONDAY__CREATE_BOARD",
