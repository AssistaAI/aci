{
  "name": "ATTIO",
  "display_name": "Attio",
  "logo": "https://logo.clearbit.com/attio.com",
  "provider": "Attio",
  "version": "1.0.0",
  "description": "\"Manage contacts, track deals, and strengthen customer relationships effortlessly through easy chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ ATTIO_CLIENT_ID }}",
      "client_secret": "{{ ATTIO_CLIENT_SECRET }}",
      "scope": "record_permission object_configuration_permission user_management workspace_permission",
      "authorize_url": "https://app.attio.com/authorize",
      "access_token_url": "https://app.attio.com/oauth/token",
      "refresh_token_url": "https://app.attio.com/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "CRM & Support",
    "Business Intelligence"
  ],
  "visibility": "public",
  "active": true
}
