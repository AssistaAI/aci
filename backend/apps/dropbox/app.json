{
  "name": "DROPBOX",
  "display_name": "Dropbox",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/dropbox.svg",
  "provider": "Dropbox, Inc.",
  "version": "1.0.0",
  "description": "\"Store files, share folders, and access documents from anywhere using simple chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_DROPBOX_APP_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_DROPBOX_APP_CLIENT_SECRET }}",
      "scope": "files.metadata.write files.content.write",
      "authorize_url": "https://www.dropbox.com/oauth2/authorize",
      "access_token_url": "https://api.dropboxapi.com/oauth2/token",
      "refresh_token_url": "https://api.dropboxapi.com/oauth2/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "File Storage",
    "Cloud Storage",
    "Collaboration",
    "Productivity"
  ],
  "visibility": "public",
  "active": true
}