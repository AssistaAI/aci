{
  "name": "GOOGLE_DOCS",
  "display_name": "Google Docs",
  "description": "Connects Assista to the Google Docs API for document workflows.",
  "categories": [
    "productivity"
  ],
  "external_docs_url": "https://developers.google.com/docs/api",
  "authorization": {
    "type": "OAUTH2",
    "authorization_url": "https://accounts.google.com/o/oauth2/v2/auth",
    "token_url": "https://oauth2.googleapis.com/token",
    "scopes": [
      "https://www.googleapis.com/auth/documents.readonly"
    ],
    "client_id_env_name": "GOOGLE_OAUTH_CLIENT_ID",
    "client_secret_env_name": "GOOGLE_OAUTH_CLIENT_SECRET",
    "redirect_uri_env_name": "ASSISTA_OAUTH_REDIRECT_URI"
  }
}
