{
  "name": "GOOGLE_DOCS",
  "display_name": "Google Docs",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/google_docs.svg",
  "provider": "Google",
  "version": "1.0.0",
  "description": "\"Create, edit, and collaborate on documents effortlessly through chat, making teamwork and writing projects smoother.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_GOOGLE_DOCS_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_GOOGLE_DOCS_CLIENT_SECRET }}",
      "scope": "https://www.googleapis.com/auth/documents",
      "authorize_url": "https://accounts.google.com/o/oauth2/v2/auth",
      "access_token_url": "https://oauth2.googleapis.com/token",
      "refresh_token_url": "https://oauth2.googleapis.com/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Document Management"
  ],
  "visibility": "public",
  "active": true
}