{
  "name": "ZENDESK",
  "display_name": "Zendesk",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/zendesk.svg",
  "provider": "Zendesk Inc.",
  "version": "2024.1",
  "description": "\"Manage support tickets, respond to customer inquiries, and streamline help desk tasks through simple chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ ZENDESK_CLIENT_ID }}",
      "client_secret": "{{ ZENDESK_CLIENT_SECRET }}",
      "scope": "read write",
      "authorize_url": "https://{subdomain}.zendesk.com/oauth/authorizations/new",
      "access_token_url": "https://{subdomain}.zendesk.com/oauth/tokens",
      "refresh_token_url": "https://{subdomain}.zendesk.com/oauth/tokens"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Tools"
  ],
  "visibility": "public",
  "active": true
}