{
  "name": "GOOGLE_TASKS",
  "display_name": "Google Tasks",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/google_tasks.svg",
  "provider": "Google",
  "version": "2.0.0",
  "description": "The Google Tasks API allows managing tasks and task lists programmatically.",
  "security_schemes": {
    "oauth2": {
      "type": "oauth2",
      "flows": {
        "authorizationCode": {
          "authorizationUrl": "https://accounts.google.com/o/oauth2/v2/auth",
          "tokenUrl": "https://oauth2.googleapis.com/token",
          "scopes": {
            "read": "Read access",
            "write": "Write access"
          }
        }
      },
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_GOOGLE_TASKS_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_GOOGLE_TASKS_CLIENT_SECRET }}",
      "authorize_url": "https://accounts.google.com/o/oauth2/v2/auth",
      "access_token_url": "https://oauth2.googleapis.com/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Productivity"
  ],
  "visibility": "public",
  "active": true
}