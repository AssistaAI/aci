{
  "name": "LINKEDIN",
  "display_name": "LinkedIn",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/linkedin.svg",
  "provider": "LinkedIn Corporation",
  "version": "2.0.0",
  "description": "\"Post updates, manage company pages, and expand your professional network effortlessly through chat commands.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_LINKEDIN_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_LINKEDIN_CLIENT_SECRET }}",
      "scope": "openid profile w_member_social email",
      "authorize_url": "https://www.linkedin.com/oauth/v2/authorization",
      "access_token_url": "https://www.linkedin.com/oauth/v2/accessToken",
      "refresh_token_url": "https://www.linkedin.com/oauth/v2/accessToken",
      "token_endpoint_auth_method": "client_secret_post",
      "grant_type": "authorization_code"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Social Media",
    "Professional Networking",
    "Business",
    "Marketing"
  ],
  "visibility": "public",
  "active": true
}
