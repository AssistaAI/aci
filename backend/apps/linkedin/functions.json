  {
    "name": "LINKEDIN__CREATE_POST",
    "description": "Create and publish a post on LinkedIn. Supports text content with visibility settings and distribution options. Use USER_PROFILE to get the author URN.",
    "tags": [
      "posts",
      "create",
      "share",
      "content"
    ],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "POST",
      "path": "/rest/posts",
      "server_url": "https://api.linkedin.com",
      "headers": {
        "Content-Type": "application/json",
        "LinkedIn-Version": "202509",
        "X-Restli-Protocol-Version": "2.0.0"
      }
    },
    "parameters": {
      "type": "object",
      "properties": {
        "body": {
          "type": "object",
          "description": "Post content and settings",
          "properties": {
            "author": {
              "type": "string",
              "description": "The URN of the LinkedIn member or organization creating the post (e.g., 'urn:li:person:123456789' or 'urn:li:organization:987654321'). Use USER_PROFILE action to get the author URN."
            },
            "commentary": {
              "type": "string",
              "description": "The main text content of the post. Supports plain text and @-mentions (e.g., @[Name](urn:li:person:xxxx))."
            },
            "visibility": {
              "type": "string",
              "description": "Controls who can see the post. PUBLIC (visible to everyone), CONNECTIONS (connections only), LOGGED_IN (signed-in members), CONTAINER (for group/event posts)",
              "default": "PUBLIC",
              "enum": [
                "PUBLIC",
                "CONNECTIONS",
                "LOGGED_IN",
                "CONTAINER"
              ]
            },
            "distribution": {
              "type": "object",
              "description": "Distribution settings for the post",
              "properties": {
                "feedDistribution": {
                  "type": "string",
                  "description": "Feed distribution type. MAIN_FEED distributes to main feed, NONE does not distribute automatically",
                  "default": "MAIN_FEED",
                  "enum": [
                    "MAIN_FEED",
                    "NONE"
                  ]
                }
              },
              "required": [],
              "visible": [
                "feedDistribution"
              ],
              "additionalProperties": false
            },
            "lifecycleState": {
              "type": "string",
              "description": "The state of the post. PUBLISHED (post directly), DRAFT (save as draft), PUBLISH_REQUESTED (requires review)",
              "default": "PUBLISHED",
              "enum": [
                "PUBLISHED",
                "DRAFT",
                "PUBLISH_REQUESTED"
              ]
            },
            "isReshareDisabledByAuthor": {
              "type": "boolean",
              "description": "Set to true to prevent others from resharing this post",
              "default": false
            }
          },
          "required": [
            "author",
            "commentary"
          ],
          "visible": [
            "author",
            "commentary",
            "visibility",
            "distribution",
            "lifecycleState",
            "isReshareDisabledByAuthor"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "body"
      ],
      "visible": [
        "body"
      ],
      "additionalProperties": false
    }
  },
