  {
    "name": "LINKEDIN__GET_COMPANY_INFO",
    "description": "Get organizations where the authenticated user has specific roles (Administrator or Content Poster), to determine management or content posting capabilities for LinkedIn company pages.",
    "tags": [
      "company",
      "organization",
      "admin",
      "roles"
    ],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "GET",
      "path": "/v2/organizationAcls",
      "server_url": "https://api.linkedin.com"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "query": {
          "type": "object",
          "description": "Query parameters for filtering organization ACLs",
          "properties": {
            "q": {
              "type": "string",
              "description": "Query type for organization ACLs",
              "default": "roleAssignee"
            },
            "role": {
              "type": "string",
              "description": "The specific role to filter by",
              "default": "ADMINISTRATOR",
              "enum": [
                "ADMINISTRATOR",
                "DIRECT_SPONSORED_CONTENT_POSTER"
              ]
            },
            "state": {
              "type": "string",
              "description": "The approval state of the role",
              "default": "APPROVED",
              "enum": [
                "APPROVED",
                "REQUESTED"
              ]
            },
            "count": {
              "type": "integer",
              "description": "Number of results to return per page",
              "default": 10,
              "minimum": 1,
              "maximum": 100
            },
            "start": {
              "type": "integer",
              "description": "Starting index for pagination",
              "default": 0,
              "minimum": 0
            }
          },
          "required": [],
          "visible": [
            "role",
            "state",
            "count",
            "start"
          ],
          "additionalProperties": false
        }
      },
      "required": [],
      "visible": [
        "query"
      ],
      "additionalProperties": false
    }
