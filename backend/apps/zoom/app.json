{
  "name": "ZOOM",
  "display_name": "Zoom",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/zoom.svg",
  "provider": "Zoom Video Communications",
  "version": "1.0.0",
  "description": "\"Schedule video meetings, host webinars, and manage recordings effortlessly through chat for seamless remote collaboration.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_ZOOM_APP_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_ZOOM_APP_CLIENT_SECRET }}",
      "scope": "meeting:write",
      "authorize_url": "https://zoom.us/oauth/authorize",
      "access_token_url": "https://zoom.us/oauth/token",
      "refresh_token_url": "https://zoom.us/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Communication",
    "Video Conferencing",
    "Collaboration"
  ],
  "visibility": "public",
  "active": true
}