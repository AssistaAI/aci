[
  {
    "name": "MEM0__GET_MEMORIES",
    "description": "Retrieve stored memories from the Mem0 AI memory service for a specific user or by memory ID.",
    "tags": ["memory", "ai", "storage"],
    "visibility": "public",
    "active": true,
    "protocol": "rest",
    "protocol_data": {
      "method": "GET",
      "path": "/v1/memories",
      "server_url": "https://api.mem0.ai"
    },
    "parameters": {
      "type": "object",
      "properties": {
        "header": {
          "type": "object",
          "properties": {
            "x-api-key": {
              "type": "string",
              "description": "API key used to authenticate requests to the Mem0 service."
            }
          },
          "required": ["x-api-key"],
          "visible": ["x-api-key"],
          "additionalProperties": false
        },
        "query": {
          "type": "object",
          "properties": {
            "user_id": {
              "type": "string",
              "description": "Return memories for a specific user."
            },
            "memory_id": {
              "type": "string",
              "description": "Return a specific memory by ID."
            },
            "limit": {
              "type": "integer",
              "description": "Maximum number of memories to retrieve."
            },
            "offset": {
              "type": "integer",
              "description": "Number of memories to skip before returning results."
            }
          },
          "required": [],
          "visible": ["user_id", "memory_id", "limit", "offset"],
          "additionalProperties": false
        }
      },
      "required": ["header"],
      "visible": ["header", "query"],
      "additionalProperties": false
    }
  }
]
