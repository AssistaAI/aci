{
  "name": "TYPEFORM",
  "display_name": "Typeform",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/typeform.svg",
  "provider": "Typeform Inc.",
  "version": "2024.1",
  "description": "\"Create surveys, design forms, and gather responses effortlessly through chat to engage your audience and simplify data collection.\"",
  "security_schemes": {
    "oauth2": {
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ TYPEFORM_CLIENT_ID }}",
      "client_secret": "{{ TYPEFORM_CLIENT_SECRET }}",
      "scope": "accounts:read forms:read responses:read forms:write",
      "authorize_url": "https://api.typeform.com/oauth/authorize",
      "access_token_url": "https://api.typeform.com/oauth/token",
      "refresh_token_url": "https://api.typeform.com/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Tools"
  ],
  "visibility": "public",
  "active": true
}