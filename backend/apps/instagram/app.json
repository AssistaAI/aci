      "client_secret": "{{ AIPOLABS_INSTAGRAM_CLIENT_SECRET }}",
      "scope": "instagram_basic instagram_manage_comments instagram_manage_insights instagram_content_publish instagram_manage_messages pages_read_engagement pages_manage_metadata",
      "authorize_url": "https://www.facebook.com/v20.0/dialog/oauth",
      "access_token_url": "https://graph.facebook.com/v20.0/oauth/access_token",
      "refresh_token_url": "https://graph.facebook.com/v20.0/oauth/access_token"
    }
  },
  "default_security_credentials_by_scheme": {},
