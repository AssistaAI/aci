{
  "name": "CLICKUP",
  "display_name": "ClickUp",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/clickup.png",
  "provider": "ClickUp",
  "version": "4.0.0",
  "description": "The ClickUp API allows you to interact with ClickUp programmatically, including workspace management, tasks, lists, folders, and more.",
  "security_schemes": {
    "oauth2": {
      "type": "oauth2",
      "flows": {
        "authorizationCode": {
          "authorizationUrl": "https://app.clickup.com/api",
          "tokenUrl": "https://api.clickup.com/api/v2/oauth/token",
          "scopes": {
            "read": "Read access",
            "write": "Write access"
          }
        }
      },
      "location": "header",
      "name": "Authorization",
      "prefix": "Bearer",
      "client_id": "{{ AIPOLABS_CLICKUP_APP_CLIENT_ID }}",
      "client_secret": "{{ AIPOLABS_CLICKUP_APP_CLIENT_SECRET }}",
      "authorize_url": "https://app.clickup.com/api",
      "access_token_url": "https://api.clickup.com/api/v2/oauth/token"
    }
  },
  "default_security_credentials_by_scheme": {},
  "categories": [
    "Productivity"
  ],
  "visibility": "public",
  "active": true
}